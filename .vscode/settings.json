// Place your settings in this file to overwrite default and user settings.
{
    "files.exclude": {
        "src/**/.js": {"when": "$(basename).ts"},
        "src/**/.js.map": true,
        "src/**/.ngfactory.ts": {"when": "$(basename).ts"},
        "src/**/.ngsummary.json": true,
        "src/**/.d.ts": {"when": "$(basename).ts"},
        "src/**/.metadata.json": true
    }
}